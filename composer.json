{
    "name": "bilogic/orchidexamples",
    "description": "Examples for the Rapid Application Development @orchidsoftware",
    "license": "MIT",
    "authors": [
        {
            "name": "bilogic",
            "homepage": "https://github.com/bilogic"
        }
    ],
    "homepage": "https://github.com/bilogic/orchidexamples",
    "keywords": [
        "Laravel",
        "Orchid",
        "Examples"
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Bilogic\\OrchidExamples\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bilogic\\OrchidExamples\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bilogic\\OrchidExamples\\OrchidExamplesServiceProvider"
            ],
            "aliases": {
                "OrchidExamples": "Bilogic\\OrchidExamples\\Facades\\OrchidExamples"
            }
        }
    }
}